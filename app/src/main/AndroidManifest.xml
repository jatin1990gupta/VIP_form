<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gform">

    <application
        android:allowBackup="true"
        android:icon="@drawable/abcd"
        android:label="@string/app_name"
        android:roundIcon="@drawable/abcd"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Splash_screen"
            android:label="@string/title_activity_splash_screen"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".submission"
            android:label="@string/title_activity_submission"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".input_data"
            android:label="@string/title_activity_input_data"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".form_details"
            android:label="@string/title_activity_form_details"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
    </application>

</manifest>